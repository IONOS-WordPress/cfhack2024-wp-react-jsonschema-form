          "type": "object",
          "additionalProperties": false,
          "properties": {
            "progress": {
              "type": "object",
              "properties": {
                "weight": {
                  "type": "number"
                },
                "caption": {
                  "type": "string"
                }
              },
              "additionalProperties": false
            },
            "step": {
              "type": "string",
              "const": "defineSiteUrl"
            },
