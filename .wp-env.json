{
  "core": "WordPress/WordPress#6.4.3",
  "phpVersion": "8.3",
  "plugins": [
    "WordPress/gutenberg#v17.8.2",
    "./packages/wp-content/plugins/gutenberg-block-attributes-jsonschema/",
    "./packages/wp-content/plugins/gutenberg-editor-sidebar-plugin/",
    "./packages/wp-content/plugins/gutenberg-form-block-jsonschema/",
    "./packages/wp-content/plugins/gutenberg-metabox-jsonschema/",
    "./packages/wp-content/plugins/settings-page/",
    "./packages/wp-content/plugins/rjsf-renderer/",
    "./packages/wp-content/plugins/rjsf-renderer-playground"
  ],
  "themes": [
    "https://downloads.wordpress.org/theme/twentytwentyfour.1.0.zip",
    "./packages/wp-content/themes/theme-extending-global-styles-jsonschema/"
  ],
  "config": {
    "SCRIPT_DEBUG": true,
    "WP_DEBUG": true,
    "WP_DEBUG_DISPLAY": false,
    "WP_DEBUG_LOG": true,
    "SAVEQUERIES": true,
    "FS_METHOD": "direct",
    "ALTERNATE_WP_CRON": true,
    "WP_DEVELOPMENT_MODE": "all"
  },
  "mappings": {
    "wp-cli.yml": "./wp-cli.wp-env.yml"
  },
  "lifecycleScripts": {
    "afterStart": "$(command -v bash) ./.wp-env-afterStart.sh",
    "afterDestroy": "$(command -v bash) -c 'rm -rf $(dirname $(pnpm run --silent wp-env install-path 2> /dev/null))'"
  }
}
